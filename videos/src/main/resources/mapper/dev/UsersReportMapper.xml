<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.guo.videos.dev.pojo.mapper.UsersReportMapper">
    <resultMap id="BaseResultMap" type="com.guo.videos.dev.pojo.UsersReport">
        <id column="id" jdbcType="VARCHAR" property="id" />
        <result column="deal_user_id" jdbcType="VARCHAR" property="dealUserId" />
        <result column="deal_video_id" jdbcType="VARCHAR" property="dealVideoId" />
        <result column="title" jdbcType="VARCHAR" property="title" />
        <result column="content" jdbcType="VARCHAR" property="content" />
        <result column="userid" jdbcType="VARCHAR" property="userId" />
        <result column="create_date" jdbcType="TIMESTAMP" property="createDate" />
    </resultMap>
    <insert id="insertOne" parameterType="com.guo.videos.dev.pojo.UsersReport">
        insert into users_report values(#{id},#{dealUserId},#{dealVideoId},#{title},#{content},#{userId},#{createDate})
    </insert>
</mapper>